<!-- Busqueda Video -->

<soapenv:Envelope
	xmlns:q0="http://localhost:8080/server"
	xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<soapenv:Body>
		<q0:SearchVideo>
			<q0:year></q0:year>
			<q0:month></q0:month>
			<q0:day></q0:day>
			<q0:title></q0:title>
			<q0:author></q0:author>
		</q0:SearchVideo>
	</soapenv:Body>
</soapenv:Envelope>

